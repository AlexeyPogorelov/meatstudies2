		section.works
			.section-label
					img(src="img/icons/side-labels/works.svg", alt="works")
			.container
				mixin work-case(name, image, description, autor)
					li.work
						a(href="#")
							.image-container
								.image-holder
									img.fake-img(src= image, alt= name)
									.img(style= 'background-image:url(' + image + ')')
							.description-container
								h3= name
								p
									b= description
								hr
								p= autor
				h3.label SOLO
				ul.work-case-holder
					+work-case(
						'The Divan',
						'http://ramilrama.com/img/portfolio/portfolio-bg-360-1.png',
